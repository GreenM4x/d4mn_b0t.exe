{
	"name": "green-bot",
	"version": "1.0.0",
	"description": "This is a Discord Bot",
	"main": "index.ts",
	"exports": "./dist/index.js",
	"type": "module",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "nodemon --watch ./**/*.*s --exec node --no-warnings --loader ts-node/esm ./index.ts",
		"build": "rm -rf dist && tsc --project tsconfig.production.json",
		"start": "node --no-warnings ./dist/index.js",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"predeploy": "npm run build",
		"deploy": "node --no-warnings dist/deploy-commands.js"
	},
	"keywords": [
		"discord"
	],
	"author": "GreenM4x",
	"license": "ISC",
	"dependencies": {
		"axios": "^1.7.7",
		"discord.js": "^14.8.0",
		"dotenv": "^16.0.3",
		"fuse.js": "^7.0.0",
		"lavalink-client": "^2.1.7",
		"node-fetch": "^3.3.2",
		"seedrandom": "^3.0.5",
		"shoukaku": "^4.0.1"
	},
	"devDependencies": {
		"@types/seedrandom": "^3.0.8",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"nodemon": "^3.1.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.5"
	}
}
